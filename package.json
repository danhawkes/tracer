{
  "name": "Tracer",
  "description": "Record where you've been, and upload tracks to other services.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/danhawkes/tracer-app.git"
  },
  "author": {
    "name": "Dan Hawkes"
  },
  "private": true,
  "dependencies": {
    "bower": "^1.3.12",
    "browser-sync": "^2.4.0",
    "del": "^1.1.1",
    "gulp": "git://github.com/gulpjs/gulp.git#4.0",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-filter": "^2.0.2",
    "gulp-imagemin": "^2.2.1",
    "gulp-load-plugins": "^0.9.0",
    "gulp-merge": "^0.1.0",
    "gulp-minify-css": "^1.0.0",
    "gulp-minify-html": "^1.0.1",
    "gulp-renvy": "0.0.3",
    "gulp-vulcanize": "^5.0.0",
    "merge-stream": "^0.1.7",
    "minimist": "^1.1.1"
  },
  "scripts": {
    "prepublish": "./node_modules/bower/bin/bower install --production --interactive=false && ./node_modules/.bin/gulp build"
  },
  "engines": {
    "node": "^0.10.0"
  },
  "firefoxManifest": {
    "launch_path": "/index.html",
    "default_locale": "en",
    "icons": {
      "128": "/images/icon-128.png",
      "512": "/images/icon-512.png"
    },
    "permissions": {
      "geolocation": {
        "description": "Required to find your location."
      }
    },
    "type": "web"
  }
}
